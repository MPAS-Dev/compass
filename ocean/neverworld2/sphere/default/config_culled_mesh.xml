<?xml version="1.0"?>
<config case="culled_mesh">

	<add_link source="../base_mesh/base_mesh.nc" dest="base_mesh.nc"/>
	<add_link source_path="script_test_dir" source="add_cullCell.py" dest="add_cullCell.py"/>

	<run_script name="run.py">
		<step executable="./add_cullCell.py">
			<argument flag="-i">base_mesh.nc</argument>
		</step>
		<step executable="MpasCellCuller.x">
			<argument flag="">base_mesh.nc</argument>
			<argument flag="">culled_mesh.nc</argument>
		</step>
		<step executable="MpasMeshConverter.x">
			<argument flag="">culled_mesh.nc</argument>
			<argument flag="">mesh.nc</argument>
		</step>
	</run_script>

</config>
